{
  "compilerOptions": {
    
    "experimentalDecorators": true,
   "inlineSourceMap": true,
   "inlineSources": true,
   "esModuleInterop": true,
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": ["node_modules"]

}
